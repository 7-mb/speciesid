{
  "expo": {
    "name": "SpeciesID",
    "slug": "speciesid",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#006268"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.example.speciesid",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Diese App benötigt Zugriff auf deine Fotomediathek, damit du Bilder auswählen kannst.",
        "NSPhotoLibraryAddUsageDescription": "Diese App speichert bearbeitete Bilder (inkl. EXIF-Dummy-Daten) in deiner Fotogalerie.",
        "NSCameraUsageDescription": "Diese App benötigt Zugriff auf die Kamera, damit du ein Foto aufnehmen kannst.",
        "UIFileSharingEnabled": true,
        "LSSupportsOpeningDocumentsInPlace": true,
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.ACCESS_MEDIA_LOCATION",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.CAMERA",
        "android.permission.ACCESS_MEDIA_LOCATION",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.READ_EXTERNAL_STORAGE"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#006268"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.example.speciesid"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "f8018928-8fd0-467d-a1f8-83ee433e4488"
      }
    },
    "plugins": ["./plugins/withAsyncStorageLocalRepo"]
  }
}
